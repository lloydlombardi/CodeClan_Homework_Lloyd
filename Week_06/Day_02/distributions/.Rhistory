fun = ~dnorm(.,
mean = samsung_stats$mean_rating,
sd = samsung_stats$sd_rating)
)
samsung_sd_1 <- samsung_mobile %>%
filter(brand == "Samsung") %>%
mutate(sd_rating = rating - samsung_stats$mean_rating) %>%
filter(sd_rating <= samsung_stats$sd_rating) %>%
summarise(proportion_sd_1 = n() / nrow(samsung_mobile))
